<?xml version="1.0"?>
<config >
	<modules >
		<Genmato_Core >
			<version >1.0.0</version >
		</Genmato_Core >
	</modules >
	<global >

		<blocks >
			<genmato_core >
				<class >Genmato_Core_Block</class >
			</genmato_core >
		</blocks >

		<helpers >
			<genmato_core >
				<class >Genmato_Core_Helper</class >
			</genmato_core >
		</helpers >

		<models >
			<genmato_core >
				<class >Genmato_Core_Model</class >
			</genmato_core >
		</models >

		<resources >
			<genmato_core_setup >
				<setup >
					<module >Genmato_Core</module >
					<class >Genmato_Core_Model_Resource_Setup</class >
				</setup >
				<connection >
					<use >core_setup</use >
				</connection >
			</genmato_core_setup >
		</resources >

	</global >

	<admin >
		<routers >
			<adminhtml >
				<args >
					<modules >
						<genmato_core after="Mage_Adminhtml" >Genmato_Core</genmato_core >
					</modules >
				</args >
			</adminhtml >
		</routers >
	</admin >

	<adminhtml >
		<events >
			<controller_action_predispatch >
				<observers >
					<genmato_core_predispatch >
						<class >genmato_core/observer</class >
						<method >preDispatch</method >
					</genmato_core_predispatch >
				</observers >
			</controller_action_predispatch >
		</events >
		<translate >
			<modules >
				<genmato_core >
					<files >
						<default >Genmato_Core.csv</default >
					</files >
				</genmato_core >
			</modules >
		</translate >
	</adminhtml >

	<default >
		<genmato_core >
			<debug >
				<enable >0</enable >
			</debug >
		</genmato_core >
	</default >

</config >